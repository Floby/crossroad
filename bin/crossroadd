#!/usr/bin/env node
// vi: ft=javascript

var Crossroad = require('../index');
var argv = require('minimist')(process.argv.slice(2));

var port = argv.port;
var crossroadd = new Crossroad({ port: port })

crossroadd.start(function () {
  console.log('instance started on port', crossroadd.port)
  var end = Date.now();
});
